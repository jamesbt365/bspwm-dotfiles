super + Return
	kitty

super + p
	rofi -show-icons -show drun

Print
	flameshot gui

super + Escape
	pkill -USR -x sxhkd

super + {_,shift + w }w
	bspc node -{c,k}

super + {t,s,f}
	bspc node -t {tiled,floating,fullscreen}

super + ctrl + n
	bspc monitor -f next

super + {_,shift + }{1-9,0}
	bspc {desktop -f,node -d} '^{1-9,10)}'

super + {_,shift + }c
	bspc node -f {next,prew}.local.!hidden.window

super + shift + {q,r}
	bspc {quit,wm -r}

